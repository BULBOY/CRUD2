<!DOCTYPE html>
<html>
<head>
<title>Table Toggle</title>
</head>
<body>

<button onclick="showTable1()">Show Table 1</button>
<button onclick="showTable2()">Show Table 2</button>

<table id="table1" style="display: none;">
  <thead>
    <tr>
      <th>Column 1</th>
      <th>Column 2</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Data 1</td>
      <td>Data 2</td>
    </tr>
    </tbody>
</table>

<table id="table2" style="display: none;">
  <thead>
    <tr>
      <th>Column A</th>
      <th>Column B</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Data A</td>
      <td>Data B</td>
    </tr>
    </tbody>
</table>

<script>
function showTable1() {
  document.getElementById("table1").style.display = "block";
  document.getElementById("table2").style.display = "none"; // Hide the other table
}

function showTable2() {
  document.getElementById("table2").style.display = "block";
  document.getElementById("table1").style.display = "none"; // Hide the other table
}
</script>

</body>
</html>
